{
  "name": "covid-buddy",
  "version": "1.0.0",
  "description": "TO add",
  "main": "index.js",
  "scripts": {
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "start": "node index.js",
    "start-server-app": "cd client && yarn build && cd .. && yarn run start",
    "test": "jest",
    "test-client": "cd client && yarn test --watchAll=false && cd ..",
    "test-api": "jest server/src/__tests__/api --watchAll=false",
    "test-server": "jest server/src/__tests__ --watchAll=false",
    "test-e2e": "jest __tests__/e2e --config=jest.e2e.config.js --watchAll=false --detectOpenHandles",
    "debug-test-e2e": "PWDEBUG=1 npm run test-e2e",
    "test-all": "npm run test-client && npm run test-api && && npm run test-server && npm run test-e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nongnoochr/covid-buddy.git"
  },
  "author": "Nongnooch Roongpiboonsopit",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nongnoochr/covid-buddy/issues"
  },
  "homepage": "https://github.com/nongnoochr/covid-buddy#readme",
  "dependencies": {
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs-node": "^3.7.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.10",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "jest": "^27.5.1",
    "jest-playwright-preset": "^1.7.0",
    "jest-when": "^3.5.1",
    "playwright": "^1.13.1",
    "supertest": "^6.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "index.js",
      "server/**/*.js",
      "!<rootDir>/node_modules/"
    ]
  }
}
